{
  "formatVersion": 1,
  "menu": {
    "global": {
      "label": "Config.Global",
      "formFields": {
        "global.mode": {
          "type": "selectBox",
          "required": true,
          "label": "Config.GlobalMode",
          "options": {
            "defaultValue": "DEMO",
            "selectBoxValues": [
              {
                "value": "DEMO",
                "caption": "Config.GlobalModeDemo"
              },
              {
                "value": "FINAL",
                "caption": "Config.GlobalModeFinal"
              }
            ]
          }
        },
        "global.container": {
          "type": "verticalContainer",
          "label": "Config.GlobalContainerLabel",
          "options": {
            "containerEntries": {
              "global.container.webserviceDemoUri": {
                "type": "inputText",
                "label": "Config.GlobalDemoWebserviceUri",
                "required": true,
                "options": {
                  "defaultValue": "https://wsdemo.ax4.com/ws/GeneralOvernight/SendungsDaten"
                }
              },
              "global.container.webserviceFinalUri": {
                "type": "inputText",
                "label": "Config.GlobalFinalWebserviceUri",
                "required": true,
                "options": {
                  "defaultValue": "https://webservice.ax4.com/ws/GeneralOvernight/SendungsDaten"
                }
              }
            }
          }
        },
        "global.username": {
          "type": "inputText",
          "required": true,
          "label": "Config.GlobalUsername",
          "options": {
            "defaultValue": ""
          }
        },
        "global.password": {
          "type": "inputText",
          "required": true,
          "label": "Config.GlobalPassword",
          "options": {
            "isPassword": true,
            "defaultValue": ""
          }
        }
      }
    },
    "sender": {
      "label": "Config.Sender",
      "formFields": {
        "sender.senderId": {
          "type": "inputText",
          "required": true,
          "label": "Config.SenderAX4ID",
          "options": {
            "tooltip": "Config.SenderAX4IDTooltip",
            "defaultValue": ""
          }
        },
        "sender.senderName": {
          "type": "inputText",
          "required": true,
          "label": "Config.SenderName",
          "options": {
            "defaultValue": ""
          }
        },
        "sender.senderStreet": {
          "type": "inputText",
          "required": true,
          "label": "Config.SenderStreet",
          "options": {
            "defaultValue": ""
          }
        },
        "sender.senderNo": {
          "type": "inputText",
          "required": true,
          "label": "Config.SenderNo",
          "options": {
            "defaultValue": ""
          }
        },
        "sender.senderPostalCode": {
          "type": "inputText",
          "required": true,
          "label": "Config.SenderPostalCode",
          "options": {
            "defaultValue": ""
          }
        },
        "sender.senderTown": {
          "type": "inputText",
          "required": true,
          "label": "Config.SenderTown",
          "options": {
            "defaultValue": ""
          }
        },
        "sender.senderCountry": {
          "type": "selectBox",
          "required": true,
          "label": "Config.SenderCountry",
          "options": {
            "defaultValue": "DE",
            "selectBoxValues": [
              {
                "value": "DE",
                "caption": "Config.SenderCountryDE"
              }
            ]
          }
        }
      }
    },
    "shipping": {
      "label": "Config.Shipping",
      "formFields": {
        "shipping.pickupTimeFrom": {
          "type": "inputText",
          "label": "Config.ShippingPickupTimeFrom",
          "options": {
            "defaultValue": "15:00"
          }
        },
        "shipping.pickupTimeTo": {
          "type": "inputText",
          "label": "Config.ShippingPickupTimeTo",
          "options": {
            "defaultValue": "18:00"
          }
        },
        "shipping.pickupNotice": {
          "type": "inputText",
          "label": "Config.ShippingPickupNotice",
          "options": {
            "defaultValue": ""
          }
        },
        "shipping.deliveryNotice": {
          "type": "inputText",
          "label": "Config.ShippingDeliveryNotice",
          "options": {
            "defaultValue": ""
          }
        },
        "shipping.enableSaturdayDelivery": {
          "type": "checkBox",
          "label": "Config.ShippingEnableSaturdayDelivery"
        }
      }
    }
  }
}